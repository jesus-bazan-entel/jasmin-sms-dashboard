{"ast":null,"code":"import{createSlice}from\"@reduxjs/toolkit\";const initialState={connectors:[],currentConnector:null,loading:false,error:null};const connectorsSlice=createSlice({name:\"connectors\",initialState,reducers:{setLoading:(state,action)=>{state.loading=action.payload;},setConnectors:(state,action)=>{state.connectors=action.payload;state.loading=false;},addConnector:(state,action)=>{state.connectors.push(action.payload);},updateConnector:(state,action)=>{const index=state.connectors.findIndex(c=>c.id===action.payload.id);if(index!==-1)state.connectors[index]={...state.connectors[index],...action.payload};},deleteConnector:(state,action)=>{state.connectors=state.connectors.filter(c=>c.id!==action.payload);},setCurrentConnector:(state,action)=>{state.currentConnector=action.payload;},setError:(state,action)=>{state.error=action.payload;state.loading=false;},clearError:state=>{state.error=null;}}});export const{setLoading,setConnectors,addConnector,updateConnector,deleteConnector,setCurrentConnector,setError,clearError}=connectorsSlice.actions;export default connectorsSlice.reducer;","map":null,"metadata":{},"sourceType":"module","externalDependencies":[]}